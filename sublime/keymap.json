[
    { "keys": ["super+b"], "command": "goto_definition" },
    { "keys": ["super+shift+k"], "command": "jump_back" },
    { "keys": ["suer+shift+j"], "command": "jump_forward" },
    {
        "keys": ["ctrl+shift+t"], "command": "terminus_open",
        "args": { "cwd": "${file_path:${folder}}" }
    },
    { "keys": ["super+g"], "command": "show_overlay", "args": {"overlay": "goto", "text": ":"} },
    { "keys": ["alt+backquote"], "command": "show_panel", "args": {"panel": "console", "toggle": true} },
    { "keys": ["ctrl+backquote"], "command": "toggle_terminus_panel" },
    {
        "keys": ["enter"], "command": "commit_completion", "context": [
            { "key": "auto_complete_visible" }
        ]
    },
    { "keys": ["ctrl+]"], "command": "next_view" },
    { "keys": ["ctrl+["], "command": "prev_view" }
]

