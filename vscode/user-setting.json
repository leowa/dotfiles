{
    "editor.fontSize": 18,
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "editor.suggestSelection": "first",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.formatOnSaveTimeout": 10000,
    "editor.formatOnSave": true,
    "vim.useCtrlKeys": true,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontFamily": "Noto Mono for Powerline",
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "workbench.editor.showTabs": false,
    "workbench.colorTheme": "Base16 Dark Monokai",
    "workbench.colorCustomizations": {
        "statusBar.background": "#1A1A1A",
        "statusBar.noFolderBackground": "#212121",
        "statusBar.debuggingBackground": "#263238"
    },
    "window.nativeTabs": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": false,
    "files.insertFinalNewline": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "git.countBadge": "off",
    "html.format.enable": false,
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "go.testFlags": [
        "-v"
    ],
    "[go]": {
        "editor.insertSpaces": true,
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.formatOnSave": false
    },
    "[python]": {
        "editor.formatOnSave": true
    },
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "java.format.enabled": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "latex-workshop.latex.build.forceRecipeUsage": true,
    "latex-workshop.latex.tools": [
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        }
    ],
    "latex-workshop.message.update.show": false,
    "latex-workshop.latex.autoBuild.run": "onFileChange",
    "security.workspace.trust.untrustedFiles": "open",
    "redhat.telemetry.enabled": true,
    "files.associations": {
        "*.jina": "yaml"
    },
    "python.formatting.blackArgs": [
        "--config=${env:HOME}/.config/pyproject.toml"
    ],
    "python.linting.flake8Args": [
        "--config=${env:HOME}/.config/flake8"
    ],
    "python.defaultInterpreterPath": "${env:HOME}/dev/py39/bin/python",
    "python.languageServer": "Pylance",
    "python.analysis.extraPaths": [
        "${workspaceFolder}"
    ],
    "editor.guides.indentation": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "editor.unicodeHighlight.allowedCharacters": {
        " ": true,
        "−": true
    },
    "diffEditor.ignoreTrimWhitespace": false
}
